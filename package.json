{
  "name": "opencode-plugin-collection",
  "version": "1.0.0",
  "description": "A collection of OpenCode plugins and commands",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build index.ts --outdir dist --target node",
    "prepare": "bun run build",
    "prepublishOnly": "bun run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "opencode",
    "plugin",
    "commands",
    "code-simplifier",
    "refactoring",
    "code-quality",
    "ai"
  ],
  "author": "Ken Huang",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenhuang/opencode-plugin-collection.git"
  },
  "bugs": {
    "url": "https://github.com/kenhuang/opencode-plugin-collection/issues"
  },
  "homepage": "https://github.com/kenhuang/opencode-plugin-collection#readme",
  "peerDependencies": {
    "@opencode-ai/plugin": ">=1.0.0"
  },
  "devDependencies": {
    "@opencode-ai/plugin": "^1.2.10",
    "@types/bun": "^1.3.1",
    "typescript": "^5.9.3"
  }
}
